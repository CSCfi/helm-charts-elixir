{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
        "auth": {
            "properties": {
                "env_basegroup": {
                    "type": "string"
                },
                "env_subgroup": {
                    "type": "string"
                },
                "mode": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "ftp": {
            "properties": {
                "classic_ftp_secret": {
                    "type": "string"
                },
                "hostip": {
                    "type": "string"
                },
                "netrc_secret": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "host_name": {
            "type": "string"
        },
        "ingress": {
            "properties": {
                "annotations": {
                    "properties": {
                        "cert-manager.io/cluster-issuer": {
                            "type": "string"
                        },
                        "kubernetes.io/tls-acme": {
                            "type": "boolean"
                        }
                    },
                    "type": "object"
                },
                "ingressClassName": {
                    "type": "string"
                },
                "path": {
                    "type": "string"
                },
                "rules": {
                    "type": "boolean"
                },
                "tls_secret_name": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "service": {
            "properties": {
                "node_port": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "storage": {
            "type": "string"
        },
        "storageClass": {
            "type": "string"
        },
        "tesk": {
            "properties": {
                "debug": {
                    "type": "boolean"
                },
                "executor_retries": {
                    "type": "integer"
                },
                "filer_retries": {
                    "type": "integer"
                },
                "image": {
                    "type": "string"
                },
                "limitsCpu": {
                    "type": "string"
                },
                "limitsMemory": {
                    "type": "string"
                },
                "port": {
                    "type": "integer"
                },
                "requestsCpu": {
                    "type": "string"
                },
                "requestsMemory": {
                    "type": "string"
                },
                "securityContext": {
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    },
                    "type": "object"
                },
                "taskmaster_filer_image_name": {
                    "type": "string"
                },
                "taskmaster_filer_image_version": {
                    "type": "string"
                },
                "taskmaster_image_name": {
                    "type": "string"
                },
                "taskmaster_image_version": {
                    "type": "string"
                },
                "tes_api_base_path": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "transfer": {
            "properties": {
                "active": {
                    "type": "boolean"
                },
                "pvc_name": {
                    "type": "string"
                },
                "tes_base_path": {
                    "type": "string"
                },
                "wes_base_path": {
                    "type": "string"
                }
            },
            "type": "object"
        }
    },
    "required": [
        "host_name",
        "storage",
        "tesk",
        "transfer",
        "auth",
        "service",
        "ftp",
        "ingress"
      ],
    "type": "object"
}
